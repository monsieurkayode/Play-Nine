{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "arrowFunctions": true,
        "modules": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "comma-dangle": 0,
      "consistent-return":  0,
      "one-var": 0,
      "one-var-declaration-per-line": 0,
      "no-bitwise": 0,
      "no-plusplus": 0
    }
}